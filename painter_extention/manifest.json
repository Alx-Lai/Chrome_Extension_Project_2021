{
	"manifest_version" : 2,
	"name" : "Painter",
	"description" : "Paint",
	"version" : "1.0",
	"browser_action" : {
		"default_title" : "Paint",
		"default_icon" : "icon.png"	
	},
	"content_scripts" : [
		{
			"matches" : ["https://*/*"],
			"js" : ["content.js"],
			"css" : ["content.css"]
		}
	],
	"permissions" : ["activeTab"]
}